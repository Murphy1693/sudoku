{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "./dist/",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "allowUmdGlobalAccess": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
  },
  // "include": ["./src/components/*.tsx", "./src/index.tsx", "./src/utils/.*ts"],
  "include": ["./src/*.tsx", "./src/*.ts"],

}
